declare type MarkInvalidationStrategy =
  | "inside"
  | "never"
  | "overlap"
  | "surround"
  | "touch"

declare interface MarkOptions {
  reversed?: boolean;
  persistent?: boolean;
  invalidate?: MarkInvalidationStrategy;
}

declare interface MarkOptionsWithHistory extends MarkOptions {
  maintainHistory?: boolean;
}
