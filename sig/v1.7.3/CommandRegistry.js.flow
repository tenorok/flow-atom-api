// -*- mode: javascript; -*- // @flow

import type { Disposable } from "./Disposable.js.flow";

declare class CommandRegistry {
  add(
    target: string,
    commandNameOrMap:
      string
      | { [commandName: string]: (event: Event) => void },
    callback?: (event: Event) => void,
  ): ?Disposable;

  dispatch(target: any, commandName: string): void;

  findCommands(props: { target: any }):
    Array<{
      name: string,
      displayName: string,
    }>;

  onDidDispatch(callback: (event: Event) => void): void;

  onWillDispatch(callback: (event: Event) => void): void;
}

export type { CommandRegistry };
